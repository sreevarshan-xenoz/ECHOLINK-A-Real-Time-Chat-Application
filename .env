# React App Environment Variables
# Copy values from .env.example and fill in your actual credentials
# Use .env.local for sensitive data (not committed to git)

# GitHub OAuth Configuration
REACT_APP_GITHUB_CLIENT_ID=your_github_client_id_here
REACT_APP_GITHUB_CLIENT_SECRET=your_github_client_secret_here

# Supabase Configuration
REACT_APP_SUPABASE_URL=your_supabase_url_here
REACT_APP_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# AI Service Configuration
REACT_APP_HF_TOKEN=your_huggingface_token_here

# Server Configuration
PORT=3000
SERVER_PORT=5000

# Environment
NODE_ENV=development
